   ],
   "source": [
    "##Fixing the slider bar at the bottom because the years were overlapping\n",
    "\n",
    "df = data\n",
    "\n",
    "# Initialize the Dash app\n",
    "app = dash.Dash(__name__)\n",
    "\n",
    "# App layout\n",
    "app.layout = html.Div(children=[\n",
    "    html.H1(children='Public Debt Analysis Dashboard'),\n",
    "\n",
    "    html.Div(children='''\n",
    "        Explore the relationships between economic indicators and public debt in Ireland.\n",
    "    '''),\n",
    "\n",
    "    dcc.Graph(id='public_debt_time_series'),\n",
    "\n",
    "    dcc.Slider(\n",
    "        id='year_slider',\n",
    "        min=df['Year'].min(),\n",
    "        max=df['Year'].max(),\n",
    "        value=df['Year'].max(),\n",
    "        ##Just fixing the bar at the bottom so the years aren't over lapping\n",
    "        marks={str(year): str(year)[-2:] for year in range(df['Year'].min(), df['Year'].max()+1, 5)},\n",
    "        step=None\n",
    "    ),\n",
    "\n",
    "    dcc.Dropdown(\n",
    "        id='indicator_dropdown',\n",
    "        options=[\n",
    "            {'label': 'Ireland Public Debt % of GDP', 'value': 'Ireland_Public_Debt_Pc_of_GDP'},\n",
    "            {'label': 'Ireland GDP', 'value': 'Ireland_GDP'},\n",
    "            {'label': 'Ireland Inflation', 'value': 'Ireland_Inflation'},\n",
    "        ],\n",
    "        value='Ireland_Public_Debt_Pc_of_GDP'\n",
    "    )\n",
    "])\n",
    "\n",
    "# Callback for updating the graph\n",
    "@app.callback(\n",
    "    Output('public_debt_time_series', 'figure'),\n",
    "    [Input('year_slider', 'value'),\n",
    "     Input('indicator_dropdown', 'value')]\n",
    ")\n",
    "def update_graph(selected_year, selected_indicator):\n",
    "    filtered_df = df[df['Year'] <= selected_year]\n",
    "    fig = px.line(filtered_df, x=\"Year\", y=selected_indicator, title=f\"{selected_indicator} over Time\")\n",
    "    fig.update_layout(\n",
    "    xaxis_tickangle=-45  # Rotates labels to -45 degrees\n",
    ")\n",
    "    return fig\n",
    "\n",
    "# Run the app\n",
    "if __name__ == '__main__':\n",
    "    app.run_server(debug=True)"
   ]
  },
